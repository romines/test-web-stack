  "name": "user-manager",
  "version": "1.0.0",
  "description": "full stack user manager",
  "main": "index.ts",
  "scripts": {
    "start": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "server": "nodemon --exec ts-node src/index.ts",
    "client": "npm run start --prefix client",
